{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/Debug/Catiablo.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build",
            "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
            "miDebuggerArgs": "",
            "logging": {
                "moduleLoad": false,
                "trace": false
            },
            "visualizerFile": "${workspaceFolder}/.vscode/gdb/visualizers.xml",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}